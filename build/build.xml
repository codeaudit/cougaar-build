<project name="build" default="compile">
<property name="build.xml" value="${basedir}/build/build_common.xml"/>
<property name="module" value="build"/>
<property name="codegen.disable" value="true"/>

<target name="build.files">
  <ant antfile="${build.xml}" target="build.files"/>
</target>

<target name="compile">
  <ant antfile="${build.xml}" target="compile"/>
</target>

<target name="jar">
  <ant antfile="${build.xml}" target="jar"/>
</target>

<target name="clean">
  <ant antfile="${build.xml}" target="clean"/>
</target>

<target name="extraclean">
  <ant antfile="${build.xml}" target="extraclean"/>
</target>

<target name="tags">
  <ant antfile="${build.xml}" target="tags"/>
</target>

<target name="compile.all">
  <ant antfile="${build.xml}" target="compile.all"/>
</target>

<target name="jar.all">
  <ant antfile="${build.xml}" target="jar.all"/>
</target>

<target name="clean.all">
  <ant antfile="${build.xml}" target="clean.all"/>
</target>

<target name="extraclean.all">
  <ant antfile="${build.xml}" target="extraclean.all"/>
</target>

<target name="tags.all">
  <ant antfile="${build.xml}" target="tags.all"/>
</target>
</project>
